  {\ifthenelse{\value{editor}>1\OR\ifandothers{editor}}
    {\bibstring{\thefield{editortype}s}}
    {\bibstring{\thefield{editortype}}}}
  \clearname{editor}}

\newbibmacro*{parteditstrg}{%
