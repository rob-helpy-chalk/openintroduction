% More authordate options %

\DeclareSortingScheme{cms}{
  \sort{
    \field{presort}
  }
  \sort[final]{
    \field{sortkey}
  }
  \sort{
    \name{sortname}
    \name{author}
    \name{namea}
    \name{editor}
    \name{nameb}
    \name{translator}
    \name{namec}
    \field{sorttitle}
    \field{journaltitle}
    \list{organization}
    \field{title}
  }
  \sort{
    \field{sortyear}
    \field{year}
  }
  \sort{
    \field{sorttitle}
    \field{title}
  }
  \sort{
    \field[padside=left,padwidth=4,padchar=0]{volume}
    \literal{0000}
  }
}

